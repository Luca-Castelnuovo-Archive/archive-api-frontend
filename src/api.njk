---
layout: default
title: API's
---

<section class="section">
    <div class="container is-max-desktop">
        <h1 class="title is-1">{{ title }}</h1>

        <hr/>

        {%- for api in collections.api -%}
            <h3 class="subtitle is-3">
                <a href="{{ api.url }}">{{ api.data.title }}</a> - {{ api.data.description }}
            </h3>
        {%- endfor -%}
    </div>
</section>
